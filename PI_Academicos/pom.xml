        <dependency>
            <groupId>org.jfxcore</groupId>
            <artifactId>javafx-controls</artifactId>
            <version>18-ea+1</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>ru.aslcraft.bell.oauth.discord</groupId>
            <artifactId>OAuth2Discord</artifactId>
